input {
    http {
        port => "5044"
    }
}
filter {
      mutate {
        lowercase => [ "message" ]
      }
    }
output {
        elasticsearch {
            hosts    => "https://172.22.0.2:9200"
            index    => "hello2"
	        user => "elastic"
	        password => "123456"
	        cacert => "/etc/logstash/certs/http_ca.crt"
        }
}
